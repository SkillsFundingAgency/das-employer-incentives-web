@model SFA.DAS.EmployerIncentives.Web.ViewModels.Apply.CannotApplyViewModel

@{
    ViewData["Title"] = Model.Title;
    ViewBag.GaData.Vpv = "/apply/no-eligible-apprentices";
    ViewData["ZenDeskLabels"] = "ei-noeligible";
}

<main class="govuk-main-wrapper " id="main-content" role="main">
    <div class="govuk-grid-row">
        <div class="govuk-grid-column-two-thirds">
            <h1 class="govuk-heading-l">
                @Model.Title
            </h1>
            <p class="govuk-body">
                You can only apply for new apprentices who joined your organisation from 1 August 2020 to 31 March 2021.
            </p>
            <p class="govuk-body">
                <a class="govuk-button" href="@Model.AccountHomeUrl">
                    Return to account home
                </a>
            </p>
        </div>
    </div>
</main>